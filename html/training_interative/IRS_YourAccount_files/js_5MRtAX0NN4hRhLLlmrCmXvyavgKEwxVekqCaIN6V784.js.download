          $('#pup-mobile-navbar-searchBtn').attr( 'tabIndex', 0 );
          $('#pup-mobile-navbar-infoBtn').attr( 'tabIndex', 0 );
          $('#pup-mobile-navbar-menuBtn').attr( 'tabIndex', 0 );
          $('#pup-mobile-navbar-infoBtn ul.menu.nav li a')[0].tabIndex = -1;
